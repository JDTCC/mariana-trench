[
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/Messenger;\\.<init>:.*"
      },
      {
        "constraint": "number_parameters",
        "inner": {
          "constraint": ">=",
          "value": 2
        }
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%type_of:Argument(1)%.handleMessage:(Ljava/lang/Object;)V"
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/FragmentTest;\\.add:\\(Landroid/app/Activity;.*"
      }
    ],
    "shim": {
      "callees": [
        {
          "lifecycle_of": "%type_of:Argument(1)%",
          "method_name": "activity_lifecycle_wrapper"
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/FragmentTest;\\.add:\\(Ljava/lang/Class;.*"
      }
    ],
    "shim": {
      "callees": [
        {
          "lifecycle_of": "%reflected_type_of:Argument(1)%",
          "method_name": "activity_lifecycle_wrapper"
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/ParameterMapping;\\.<init>:.*"
      },
      {
        "constraint": "number_parameters",
        "inner": {
          "constraint": "==",
          "value": 5
        }
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%type_of:Argument(2)%.handleMessage:(Ljava/lang/Object;)V",
          "parameters_map": {
            "Argument(1)": "Argument(4)"
          }
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/ParameterMapping;\\.inferred:.*"
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%type_of:Argument(1)%.multipleArguments:(ILjava/lang/String;Ljava/lang/Object;)V"
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/ParameterMapping;\\.defined:.*"
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%type_of:Argument(1)%.multipleArguments:(ILjava/lang/String;Ljava/lang/Object;)V",
          "parameters_map": {
            "Argument(3)": "Argument(2)"
          }
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/ParameterMapping;\\.callStatic:.*"
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%static:Lcom/facebook/marianatrench/integrationtests/ShimTarget;%.staticMethod:(Ljava/lang/Object;)V",
          "parameters_map": {
            "Argument(0)": "Argument(1)"
          }
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "is_static",
        "value": true
      },
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/ReflectionShims;\\.reflect:\\(Ljava/lang/Class;\\)V"
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%reflected_type_of:Argument(0)%.api:(Ljava/lang/Object;)V"
        }
      ]
    }
  },
  {
    "find": "methods",
    "where": [
      {
        "constraint": "is_static",
        "value": true
      },
      {
        "constraint": "signature",
        "pattern": "Lcom/facebook/marianatrench/integrationtests/ReflectionShims;\\.reflectWithArgument:\\(Ljava/lang/Class;Ljava/lang/Object;\\)V"
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%reflected_type_of:Argument(0)%.api:(Ljava/lang/Object;)V",
          "parameters_map": {
            "Argument(1)": "Argument(1)"
          }
        }
      ]
    }
  }
]
