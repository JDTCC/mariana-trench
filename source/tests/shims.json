[
  {
    "find": "methods",
    "where": [
      {
        "constraint": "signature",
        "pattern": "LExample;\\.methodToShim:\\(LShimmed1;\\)V"
      }
    ],
    "shim": {
      "callees": [
        {
          "signature": "%type_of:Argument(1)%.method:()V"
        },
        {
          "signature": "%static:LShimmed2;%.static_method:()V"
        }
      ]
    }
  }
]
