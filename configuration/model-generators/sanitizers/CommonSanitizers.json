{
  "model_generators": [
    {
      "find": "methods",
      "where": [
        {
          "constraint": "signature",
          "pattern": "Lkotlin/Result;.*Ljava/lang/Throwable;"
        }
      ],
      "model": {
        "sanitizers": [
          {
            "sanitize": "propagations"
          }
        ]
      }
    }
  ]
}
